{
    "name": "ekatra/product-sdk",
    "description": "Ekatra Product SDK for PHP - Transform your product data into Ekatra Co-Deciding format",
    "type": "library",
    "license": "MIT",
    "version": "1.0.5",
    "authors": [
        {
            "name": "Ekatra Global Corp",
            "email": "support@ekatraglobal.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/validation": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Ekatra\\Product\\": "src/Ekatra/Product/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ekatra\\Product\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ekatra\\Product\\Laravel\\EkatraProductServiceProvider"
            ],
            "aliases": {
                "Ekatra": "Ekatra\\Product\\Laravel\\Facades\\Ekatra"
            }
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage --coverage-clover coverage/coverage.xml",
        "test-unit": "phpunit tests/Unit",
        "test-integration": "phpunit tests/Integration"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
