{
    "name": "ekatra/product-sdk",
    "description": "Ekatra Product SDK for PHP - Transform your product data into Ekatra format",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ekatra",
            "email": "support@ekatra.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/validation": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Ekatra\\Product\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ekatra\\Product\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ekatra\\Product\\Laravel\\EkatraProductServiceProvider"
            ],
            "aliases": {
                "Ekatra": "Ekatra\\Product\\Laravel\\Facades\\Ekatra"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
